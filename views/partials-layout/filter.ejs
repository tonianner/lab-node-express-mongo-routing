<!-- Filter Panel - Left column -->
<div class="panel panel-default">
  <div class="panel-heading">
    <h1 class="panel-title">
      <span class="glyphicon glyphicon-heart"></span>
      <span class="glyphicon glyphicon-heart"></span>
      My SUPER favourites
    </h1>
  </div>

  <div class="panel-body">
    <div class="col-xs-12 filter-img">
      <% for (var i in sushi){ %>
        <% if (sushi[i].status == "SUPER favourite"){ %>
          <div>
            <img  class="img-responsive commenterImage"
                  onerror="this.src='./images/noImage.jpg'"
                  src="<%= sushi[i].photoUrl %>"
            />
          </div>
        <% } %>
      <% } %>
    </div>
  </div>
</div>

<!-- Filter Panel - Left column -->
<div class="panel panel-default">
  <div class="panel-heading">
    <h1 class="panel-title">
      <span class="glyphicon glyphicon-heart"></span>
      favourites
    </h1>
  </div>

  <div class="panel-body">
    <div class="col-xs-12 filter-img">
      <% for (var i in sushi){ %>
        <% if (sushi[i].status == "favourite"){ %>
          <div>

            <p><b><%= sushi[i].sushi_name %></b></p>
            <img  class="img-responsive commenterImage"
                  onerror="this.src='./images/noImage.jpg'"
                  src="<%= sushi[i].photoUrl %>"
            />

          </div>
        <% } %>
      <% } %>
    </div>
  </div>
</div>

